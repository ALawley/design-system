{
  "name": "@puppet/uikit",
  "version": "0.1.0",
  "main": "dist/uikit.js",
  "author": "Puppet, Inc.",
  "license": "UNLICENSED",
  "scripts": {
    "build":
      "NODE_ENV=production webpack --config=config/bundle.webpack.config.js",
    "build:styleguide":
      "(export $(cat config/.env.production | xargs) && webpack --config=config/production.webpack.config.js)",
    "precommit": "lint-staged",
    "prepare": "echo 'Building uikit' && npm run build",
    "format":
      "eslint --fix src styleguide config && prettier --write --list-different '{src,styleguide,config}/**/*.{js,jsx,mjs,graphql,json,css,md}'",
    "lint": "eslint src styleguide config --ext .js --ext .jsx",
    "start": "node --experimental-modules styleguide/server/dev.mjs",
    "serve": "node --experimental-modules styleguide/server/prod.mjs",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js, jsx, mjs}": ["eslint --fix"],
    "*.{js,jsx,mjs,graphql,json,css,md}": [
      "prettier --single-quote --trailing-comma=all --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-3": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "8.0.0-beta.2",
    "caniuse-lite": "^1.0.30000830",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.18",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "express": "^4.16.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.2",
    "mini-css-extract-plugin": "^0.4.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "query-string": "^6.1.0",
    "raw-loader": "^0.5.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.1.3",
    "react-router-dom": "^4.2.2",
    "sinon": "4.5.0",
    "sinon-chai": "^3.0.0",
    "style-loader": "^0.21.0",
    "url-search-params": "^0.10.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.2.0"
  }
}

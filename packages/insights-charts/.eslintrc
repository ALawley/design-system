{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "react/jsx-curly-spacing": [2, "always"],
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "new-cap": 0,
    "newline-before-return": 2,
    "no-param-reassign": [0, {
      "props": false
    }]
  },
  "globals": {
    "window": true,
    "document": true
  }
}

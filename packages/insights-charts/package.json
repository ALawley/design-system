  "description": "Highly customizable charting library built on top of D3",
  "main": "build/library.js",
  "scripts": {
    "prebuild": "./node_modules/.bin/rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run build",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "./node_modules/.bin/nyc --reporter=html ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive",
    "js-lint": "./node_modules/.bin/eslint --quiet 'source/js/**/*.js'",
    "scss-lint": "./node_modules/.bin/stylelint 'source/scss/**/*.scss'",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reflect/reflect-charts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reflect/reflect-charts/issues"
  },
  "homepage": "https://github.com/reflect/reflect-charts#readme",
  "dependencies": {
    "classnames": "2.2.5",
    "clone": "^2.1.1",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-ease": "^1.0.3",
    "d3-format": "^1.2.0",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.0",
    "deepmerge": "1.3.2"

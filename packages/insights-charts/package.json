{
  "name": "@puppet/insights-charts",
  "version": "0.10.0",
  "description": "Highly customizable charting library built on top of D3",
  "main": "build/library.js",
  "scripts": {
    "prebuild": "./node_modules/.bin/rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run build",
    "complexity": "./node_modules/.bin/es6-plato -r -d complexity source/js && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "./node_modules/.bin/nyc --require=./test/jsdom.js --reporter=html ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive",
    "js-lint": "./node_modules/.bin/eslint --quiet 'source/js/**/*.js'",
    "scss-lint": "./node_modules/.bin/stylelint 'source/scss/**/*.scss'",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetlabs/insights-charts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/puppetlabs/insights-charts/issues"
  },
  "homepage": "https://github.com/puppetlabs/insights-charts#readme",
  "publishConfig": {
    "registry": "https://artifactory.delivery.puppetlabs.net/artifactory/api/npm/npm__local/"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clone": "^2.1.1",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-ease": "^1.0.3",
    "d3-format": "^1.2.0",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.0",
    "d3-zoom": "^1.7.1",
    "debounce": "^1.1.0",
    "deepmerge": "1.3.2"
  },
  "devDependencies": {
    "@puppet/insights-editor": "1.0.0",
    "@puppet/react-components": "3.0.0",
    "babel-core": "6.23.1",
    "babel-eslint": "8.2.3",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "chai": "^3.5.0",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "css-loader": "^0.27.1",
    "es6-plato": "^1.0.18",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fast-csv": "^2.4.1",
    "jsdom": "11.1.0",
    "mocha": "^3.3.0",
    "moment": "^2.22.2",
    "node-sass": "4.9.0",
    "nodemon": "^1.11.0",
    "nyc": "10.2.0",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "stylelint": "8.0.0",
    "stylelint-config-sass-guidelines": "3.1.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "peerDependencies": {
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "moment": "^2.22.2"
  }
}

{
  "name": "@puppet/insights-charts",
  "version": "0.13.0",
  "description": "Highly customizable charting library built on top of D3",
  "main": "build/library.js",
  "scripts": {
    "analyze": "webpack --config webpack.analyze.config.js",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run build",
    "complexity": "es6-plato -r -d complexity source/js && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "nyc --require=./test/jsdom.js --reporter=html ./node_modules/.bin/mocha --compilers js:@babel/register --recursive",
    "js-lint": "eslint --quiet 'source/js/**/*.js'",
    "scss-lint": "stylelint 'source/scss/**/*.scss'",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "format": "eslint --fix 'source/**/*.{js,jsx}'; prettier --write 'source/**/*.{js,jsx,json,md}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetlabs/insights-charts.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/puppetlabs/insights-charts/issues"
  },
  "homepage": "https://github.com/puppetlabs/insights-charts#readme",
  "publishConfig": {
    "registry": "https://artifactory.delivery.puppetlabs.net/artifactory/api/npm/npm__local/"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "clone": "^2.1.1",
    "d3-array": "^1.2.0",
    "d3-axis": "^1.0.8",
    "d3-dispatch": "^1.0.5",
    "d3-ease": "^1.0.3",
    "d3-format": "^1.2.0",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "d3-selection": "^1.1.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.0",
    "d3-zoom": "^1.7.1",
    "debounce": "^1.1.0",
    "deepmerge": "1.3.2",
    "moment": "^2.22.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "@puppet/insights-editor": "1.0.0",
    "@puppet/react-components": "3.0.0",
    "babel-eslint": "8.2.3",
    "babel-loader": "8.0.4",
    "chai": "^3.5.0",
    "core-js": "^2.5.7",
    "css-loader": "^0.27.1",
    "enzyme": "3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-plato": "^1.0.18",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fast-csv": "^2.4.1",
    "jsdom": "11.1.0",
    "mocha": "^3.3.0",
    "node-sass": "4.9.0",
    "nodemon": "^1.11.0",
    "nyc": "10.2.0",
    "prop-types": "15.6.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "stylelint": "8.0.0",
    "stylelint-config-sass-guidelines": "3.1.0",
    "webpack": "4.8.3",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  }
}

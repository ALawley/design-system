    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.27.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-react": "6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "jsdom": "^10.0.0",
    "mocha": "^3.3.0",
    "moment": "2.17.1",
    "node-sass": "4.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.0",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.2",
    "stylelint": "8.0.0",
    "stylelint-config-sass-guidelines": "3.1.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  }

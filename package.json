{
  "name": "@puppet/react-components",
  "version": "5.0.0-alpha.12",
  "description": "Collection of reusable React components",
  "main": "build/library.js",
  "publishConfig": {
    "registry": "https://artifactory.delivery.puppetlabs.net/artifactory/api/npm/npm__local/"
  },
  "scripts": {
    "analyze": "webpack --config webpack.analyze.config.js",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run build",
    "complexity": "es6-plato -r -d complexity source/react && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "nyc --require=./test/jsdom.js --reporter=html mocha --compilers js:@babel/register --recursive",
    "js-lint": "eslint --quiet 'source/react/**/*.js'",
    "scss-lint": "stylelint 'source/scss/**/*.scss'",
    "start": "styleguidist server",
    "format": "eslint --fix 'source/**/*.{js,jsx}'; prettier --write 'source/**/*.{js,jsx,json,md}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetlabs/react-components.git"
  },
  "author": "Puppet, Inc.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/puppetlabs/react-components/issues"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "homepage": "https://github.com/puppetlabs/react-components#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "^1.0.0-rc.2",
    "core-js": "^2.5.7",
    "css-loader": "0.28.11",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "es6-plato": "^1.0.18",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-csv": "^2.4.1",
    "file-loader": "^1.1.11",
    "jsdom": "11.1.0",
    "mocha": "3.5.3",
    "mocha-jsdom": "1.2.0",
    "node-sass": "4.9.0",
    "nodemon": "^1.10.2",
    "nyc": "10.3.2",
    "prettier": "1.14.2",
    "prop-types": "^15.6.2",
    "react": "16.8.0",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "16.8.0",
    "react-router": "^3.0.0",
    "react-styleguidist": "8.0.6",
    "sass-loader": "7.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "9.1.3",
    "stylelint-config-sass-guidelines": "5.0.0",
    "webpack": "4.8.3",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "prop-types": "^15.6.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "clone": "^2.1.2",
    "debounce": "^1.1.0",
    "deep-equal": "^1.0.1",
    "hoist-non-react-statics": "^3.1.0",
    "react-onclickoutside": "^6.7.0",
    "react-sortable-hoc": "^0.6.8",
    "react-transition-group": "^2.2.1"
  }
}

{
  "name": "@puppet/react-components",
  "version": "2.1.0",
  "description": "Collection of reusable React components",
  "main": "build/library.js",
  "publishConfig": {
    "registry": "https://artifactory.delivery.puppetlabs.net/artifactory/api/npm/npm__local/"
  },
  "scripts": {
    "prebuild": "./node_modules/.bin/rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepublish": "npm run build",
    "complexity": "./node_modules/.bin/es6-plato -r -d complexity source/react && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "./node_modules/.bin/nyc --require=./test/jsdom.js --reporter=html ./node_modules/.bin/mocha --compilers js:babel-core/register --recursive",
    "js-lint": "./node_modules/.bin/eslint --quiet 'source/react/**/*.js'",
    "scss-lint": "./node_modules/.bin/stylelint 'source/scss/**/*.scss'",
    "start": "./node_modules/.bin/styleguidist server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puppetlabs/react-components.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/puppetlabs/react-components/issues"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "homepage": "https://github.com/puppetlabs/react-components#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.5",
    "css-loader": "0.28.11",
    "debounce": "1.1.0",
    "enzyme": "^2.4.1",
    "es6-plato": "^1.0.18",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-csv": "^2.4.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.1.0",
    "mocha-jsdom": "1.1.0",
    "moment-timezone": "0.5.13",
    "node-sass": "4.9.0",
    "nodemon": "^1.10.2",
    "nyc": "^10.2.0",
    "react": "15.3.2",
    "react-addons-test-utils": "15.3.2",
    "react-dom": "15.3.2",
    "react-portal": "3.0.0",
    "react-router": "^3.0.0",
    "react-styleguidist": "7.0.14",
    "sass-loader": "7.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylelint": "8.0.0",
    "stylelint-config-sass-guidelines": "3.1.0",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "peerDependencies": {
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "moment": "^2.11.2",
    "moment-timezone": "0.5.13"
  },
  "dependencies": {
    "blueimp-md5": "^2.10.0",
    "deep-equal": "^1.0.1",
    "react-daterange-picker": "^1.1.0",
    "react-onclickoutside": "^6.7.0",
    "react-sortable-hoc": "^0.6.8",
    "react-transition-group": "^2.2.1"
  }
}
